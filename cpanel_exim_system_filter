or $received_protocol is "esmtpsa")
and ("$h_to:, $h_cc:" contains "egs.com.pk")
or ("$h_from:" contains "egs.com.pk")
then
logwrite "$tod_full $message_id from $sender_address contained egs.com.pk keywords"
seen finish
